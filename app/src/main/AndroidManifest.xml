<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.quick.myfin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_fin_app"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_fin_app_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.SaveCashActivity"></activity>
        <activity android:name=".Activity.CashPlanActivity" />
        <activity android:name=".Activity.ShowAllActivity" />
        <activity android:name=".Activity.HomeActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>